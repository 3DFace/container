{
	"name": "dface/container",
	"description": "Simple IoC container for php",
	"keywords": ["Container", "Dependency injection", "Inversion of control"],
	"type": "library",
	"license": "MIT",
	"authors": [{
		"name": "Denis Ponomarev",
		"email": "ponomarev@gmail.com"
	}
	],
	"require": {
		"php": ">=7.1.0",
		"psr/container": "^1.0"
	},
	"provide": {
		"psr/container-implementation":  "1.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*"
	},
	"autoload": {
		"psr-0": {"dface\\container": "src/"}
	},
	"autoload-dev": {
		"psr-0": {"dface\\container": "tests/"}
	}
}
